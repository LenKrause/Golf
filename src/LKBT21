0100 REM "LKBT21 - TEST SELECT WHERE CLAUSE WITH CVS() AND SUB-STRING
0110 BEGIN
0120 CALL "CDS069",1,10,0,0,"+NAME","UA",NAME,NAME$
0130 CALL "CDS041","OSUM10",UM10$
0150 LET WH$="cvs(UM10.NAME$(1,"+STR(NAME)+"),4)="""+NAME$+""""

0152 REM let WH$="POS(NAME$=CVS(UM10.NAME$,4))=1"
0160 SELECT (1)UM10$ FROM "OSUM10" WHERE CPL(WH$)
0170 READ RECORD(1)UM10$
0180 PRINT UM10.NAME$
0190 GOTO 0170
