0100 REM "GW001 - 05/05/12 Return Golfer Name & Radio Selection
0110 SETESC DT_ERR; SETERR DT_ERR
0120 ENTER Y$,GR04$,GW001$
0130 LET GW001.NAME$=CVS(GR04.FIRSTNAME$,2)+" "+CVS(GR04.LASTNAME$,2)
0140 LET GW001.NAMERADIO$="<input type='radio' name='ID' value='"+GR04.ID$+"'>"+GW001.NAME$
0150 EXIT

8000 DT_ERR:
8010 CALL "CDS063",STR(TCB(5+3*(ERR=127))),Y$,PGM(-2)
8020 IF Y.ERRSTS=0 THEN SETERR 0 ELSE IF Y.ERRSTS=2 THEN RETURN
8030 RETRY

9000 EOJ:
9010 IF TCB(13) THEN EXIT
9020 RUN "CDS001"
