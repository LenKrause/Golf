0100 REM "LKS130 - 09/19/94 DISPLAY INFO() VARIABLE
0110 BEGIN
0120 LET Z$=INFO(I,J,ERR=0150)
0121 IF CVS(Z$,16)<>Z$ THEN LET Z$='SB'+"$"+'SF'+HTA(Z$)+'SB'+"$"+'SF'
0125 IF L AND MOD(L,20)=0 THEN INPUT "Enter=continue ",*
0130 PRINT "INFO(",STR(I),",",STR(J),")=",Z$
0140 LET J=J+1,L=L+1
0145 GOTO 0120
0150 IF I<10 OR J THEN LET J=0,I=I+1; GOTO 0120
